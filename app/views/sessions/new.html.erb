<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-4">
    <%= render 'shared/content_title', title: 'Log In' %>
    <div class="well">
      <%= bootstrap_form_tag url: login_path do |f| %>
        <%= f.text_field :email %>
        <%= f.password_field :password %>
        <%= f.submit 'Log In' %>
      <% end %>
    </div>
  <div class="col-md-4"></div>
</div>
